  owl:disjointWith TerraCore:Pipeline ;
  owl:disjointWith TerraCore:Sequencing ;
  owl:disjointWith TerraCore:VariantCalling ;
  owl:equivalentClass [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty TerraCore:usedReferenceAssembly ;
    ] ;
  owl:equivalentClass [
      a owl:Restriction ;
      owl:onProperty prov:generated ;
      owl:someValuesFrom TerraCore:AlignmentFile ;
    ] ;
  owl:equivalentClass [
      a owl:Restriction ;
      owl:onProperty prov:used ;
